{
  "name": "MongoMongooseNewsScraper",
  "version": "1.0.0",
  "description": "A web app that uses cheerio and axios to scrape news sites and use mongoose and mongo to store and use the srapings.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged",
        "npm test",
        "git add"
      ]
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tonythetiger323/MongoMongooseNewsScraper.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tonythetiger323/MongoMongooseNewsScraper/issues"
  },
  "homepage": "https://github.com/tonythetiger323/MongoMongooseNewsScraper#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.0",
    "express-handlebars-sections": "^1.0.1",
    "mongoose": "^5.4.5",
    "morgan": "^1.9.1",
    "trackjs": "^3.0.1"
  },
  "devDependencies": {
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.10.0"
  }
}
